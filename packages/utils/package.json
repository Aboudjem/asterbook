{
  "name": "@asterbook/utils",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @asterbook/config",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external @asterbook/config --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "dependencies": {
    "@asterbook/config": "workspace:*"
  },
  "devDependencies": {
    "@asterbook/eslint-config": "workspace:*",
    "@asterbook/types": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.3.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
